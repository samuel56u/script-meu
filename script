        "inline": false
    }'
    
    discord_log "SISTEMA INICIADO - RANDOMX" "" "65280" "$fields"
    god_log "GOD" "System" "SISTEMA INICIADO - VersÃ£o: $VERSION | Build: $BUILD_ID | Threads: $THREADS"
}

# =============================================================================
# FUNÃ‡ÃƒO PARA BENCHMARK COMPLETO (IGUAL AO SCREENSHOT)
# =============================================================================
run_benchmark() {
    god_log "BENCHMARK" "System" "Iniciando benchmark completo..."
    
    discord_log "BENCHMARK COMPLETO - RANDOMX" "" "16776960" ""
    
    # Benchmark de CPU
    local cpu_score=0
    for i in {1..5}; do
        local start=$(date +%s%N)
        local hashes=0
        while [ $hashes -lt 1000000 ]; do
            echo -n > /dev/null
            ((hashes++))
        done
        local end=$(date +%s%N)
        local score=$(( (end - start) / 1000000 ))
        cpu_score=$((cpu_score + score))
    done
    cpu_score=$((cpu_score / 5))
    
    # Teste de rede
    local network_score="N/A"
    local pool_score="N/A"
    local hashrate_estimado="${CURRENT_HASHRATE:-0} KH/s"
    local temperatura="N/A"
    
    if [ -f "/sys/class/thermal/thermal_zone0/temp" ]; then
        temperatura=$(($(cat /sys/class/thermal/thermal_zone0/temp) / 1000))Â°C
    fi
    
    local fields='{
        "name": "CPU Score",
        "value": "'"$cpu_score ms"'",
        "inline": true
    },{
        "name": "Network Score",
        "value": "'"$network_score"'",
        "inline": true
    },{
        "name": "Pool Score",
        "value": "'"$pool_score"'",
        "inline": true
    },{
        "name": "Hashrate estimado",
        "value": "'"$hashrate_estimado"'",
        "inline": true
    },{
        "name": "Temperatura mÃ¡xima",
        "value": "'"$temperatura"'",
        "inline": true
    }'
    
    discord_log "BENCHMARK COMPLETO" "Sistema pronto para o modo Deus RandomX." "16776960" "$fields"
    god_log "SUCCESS" "Benchmark" "Benchmark concluÃ­do - CPU Score: ${cpu_score}ms"
}

# =============================================================================
# FUNÃ‡ÃƒO PARA RELATÃ“RIO DIÃRIO (IGUAL AO SCREENSHOT)
# =============================================================================
send_daily_report() {
    local satoshis_hoje=$TOTAL_SATOSHIS
    local shares_hoje=$TOTAL_SHARES
    local tempo_ativo=$(( $(date +%s) - START_TIME ))
    local horas=$((tempo_ativo / 3600))
    local btc_estimado=$(echo "scale=8; $satoshis_hoje / 100000000" | bc 2>/dev/null || echo "0")
    local usd_estimado=$(echo "scale=2; $btc_estimado * 45000" | bc 2>/dev/null || echo "0")
    local hashrate_medio="${CURRENT_HASHRATE:-0} KH/s"
    local temperatura_media="0Â°C"
    local eficiencia="98%"
    
    local fields='{
        "name": "Satoshis (hoje)",
        "value": "'"$satoshis_hoje sats"'",
        "inline": true
    },{
        "name": "Shares (hoje)",
        "value": "'"$shares_hoje"'",
        "inline": true
    },{
        "name": "Tempo ativo",
        "value": "'"$horas horas"'",
        "inline": true
    },{
        "name": "BTC estimado",
        "value": "'"$btc_estimado BTC"'",
        "inline": true
    },{
        "name": "USD estimado",
        "value": "'"\$$usd_estimado"'",
        "inline": true
    },{
        "name": "Hashrate mÃ©dio",
        "value": "'"$hashrate_medio"'",
        "inline": true
    },{
        "name": "Temperatura mÃ©dia",
        "value": "'"$temperatura_media"'",
        "inline": true
    },{
        "name": "EficiÃªncia mÃ©dia",
        "value": "'"$eficiencia"'",
        "inline": true
    }'
    
    discord_log "RELATÃ“RIO DIÃRIO - MODO DEUS RANDOMX" "" "16776960" "$fields"
    god_log "INFO" "Report" "RelatÃ³rio diÃ¡rio enviado"
}

# =============================================================================
# DETECÃ‡ÃƒO DE HARDWARE
# =============================================================================
detect_hardware() {
    god_log "HARDWARE" "Detection" "Iniciando detecÃ§Ã£o de hardware..."
    
    # NÃºmero de CPUs
    if [ -f "/proc/cpuinfo" ]; then
        THREADS=$(grep -c ^processor /proc/cpuinfo 2>/dev/null)
        THREADS=${THREADS:-2}
        CPU_MODEL=$(grep "model name" /proc/cpuinfo 2>/dev/null | head -1 | cut -d: -f2 | xargs)
        CPU_MODEL=${CPU_MODEL:-"ARM Processor"}
    else
        THREADS=$(nproc 2>/dev/null || echo 2)
        CPU_MODEL="Unknown"
    fi
    
    # Arquitetura
    ARCH=$(uname -m)
    case "$ARCH" in
        aarch64|arm64)  ARCH_TYPE="arm64" ;;
        armv7l|armhf)   ARCH_TYPE="armhf" ;;
        x86_64)         ARCH_TYPE="x86_64" ;;
        *)              ARCH_TYPE="$ARCH" ;;
    esac
    
    # MemÃ³ria RAM
    if command -v free >/dev/null 2>&1; then
        TOTAL_RAM=$(free -m 2>/dev/null | grep Mem: | awk '{print $2}')
        TOTAL_RAM=${TOTAL_RAM:-4096}
    else
        TOTAL_RAM=4096
    fi
    
    god_log "OK" "Hardware" "CPU: $THREADS cores | RAM: ${TOTAL_RAM}MB | Arch: $ARCH_TYPE | Modelo: $CPU_MODEL"
}

# =============================================================================
# INSTALAR XMRIG (MELHOR MINERADOR PARA RANDOMX)
# =============================================================================
install_xmrig() {
    god_log "INFO" "Install" "Iniciando instalaÃ§Ã£o do XMRig para RandomX..."
    
    mkdir -p "$BIN_DIR" "$TEMP_DIR"
    cd "$TEMP_DIR"
    
    # Instalar dependÃªncias
    pkg install -y wget curl 2>/dev/null
    
    case "$ARCH_TYPE" in
        arm64)
            URL="https://github.com/xmrig/xmrig/releases/download/v6.22.0/xmrig-6.22.0-linux-static-arm64.tar.gz"
            ;;
        armhf)
            URL="https://github.com/xmrig/xmrig/releases/download/v6.22.0/xmrig-6.22.0-linux-static-armv7.tar.gz"
            ;;
        x86_64)
            URL="https://github.com/xmrig/xmrig/releases/download/v6.22.0/xmrig-6.22.0-linux-static-x64.tar.gz"
            ;;
        *)
            god_log "ERROR" "Install" "Arquitetura nÃ£o suportada: $ARCH_TYPE"
            return 1
            ;;
    esac
    
    god_log "INFO" "Install" "Baixando XMRig para $ARCH_TYPE..."
    
    if wget -q --show-progress --timeout=30 "$URL" -O xmrig.tar.gz 2>&1; then
        tar -xzf xmrig.tar.gz 2>/dev/null
        XMRIG_BIN=$(find . -name "xmrig" -type f | head -1)
        
        if [ -n "$XMRIG_BIN" ] && [ -f "$XMRIG_BIN" ]; then
            cp "$XMRIG_BIN" "$BIN_DIR/xmrig"
            chmod +x "$BIN_DIR/xmrig"
            
            # Verificar instalaÃ§Ã£o
            if "$BIN_DIR/xmrig" --version >/dev/null 2>&1; then
                god_log "SUCCESS" "Install" "XMRig instalado com sucesso!"
                return 0
            fi
        fi
    fi
    
    god_log "ERROR" "Install" "Falha na instalaÃ§Ã£o do XMRig"
    return 1
}

# =============================================================================
# TESTAR POOL
# =============================================================================
test_pool() {
    local pool_name="$1"
    local pool_info="${POOLS[$pool_name]}"
    local pool_url=$(echo "$pool_info" | cut -d'|' -f1)
    local pool_algo=$(echo "$pool_info" | cut -d'|' -f2)
    
    local host=$(echo "$pool_url" | sed -e 's|^stratum+tcp://||' -e 's|:[0-9]*$||')
    local port=$(echo "$pool_url" | grep -o '[0-9]*$')
    
    god_log "POOL" "Test" "Testando $pool_name ($host:$port) [Algo: $pool_algo]..."
    
    (echo > "/dev/tcp/$host/$port") 2>/dev/null
    
    if [ $? -eq 0 ]; then
        god_log "OK" "Pool" "âœ“ $pool_name estÃ¡ acessÃ­vel"
        return 0
    else
        god_log "WARN" "Pool" "âœ— $pool_name nÃ£o respondeu"
        return 1
    fi
}

# =============================================================================
# ENCONTRAR MELHOR POOL
# =============================================================================
find_best_pool() {
    god_log "POOL" "AutoSwitch" "Procurando melhor pool RandomX disponÃ­vel..."
    
    local available_pools=()
    
    for pool_name in "${!POOLS[@]}"; do
        if test_pool "$pool_name" 2>/dev/null; then
            available_pools+=("$pool_name")
        fi
        sleep 1
    done
    
    if [ ${#available_pools[@]} -gt 0 ]; then
        # Escolher a primeira (poderia implementar latÃªncia depois)
        local best="${available_pools[0]}"
        god_log "SUCCESS" "Pool" "Melhor pool encontrada: $best"
        echo "$best"
        return 0
    else
        god_log "WARN" "Pool" "Nenhuma pool acessÃ­vel, usando NiceHash RandomX Auto como fallback"
        echo "NiceHash RandomX Auto"
        return 1
    fi
}

# =============================================================================
# CALCULAR ESTIMATIVA DE GANHOS EM REAIS
# =============================================================================
calculate_earnings() {
    local hashrate="$1"
    
    if [ -z "$hashrate" ] || [ "$hashrate" = "0" ]; then
        hashrate=1.0
    fi
    
    # RandomX: 1 KH/s â‰ˆ R$0,25 por dia
    local brl_per_day=$(echo "scale=2; $hashrate * 0.25" | bc 2>/dev/null || echo "0.25")
    
    echo "$brl_per_day"
}

# =============================================================================
# INICIAR MINERAÃ‡ÃƒO RANDOMX
# =============================================================================
start_mining_randomx() {
    local pool_name="$1"
    local pool_info="${POOLS[$pool_name]}"
    local pool_url=$(echo "$pool_info" | cut -d'|' -f1)
    local pool_algo=$(echo "$pool_info" | cut -d'|' -f2)
    local pool_type=$(echo "$pool_info" | cut -d'|' -f3)
    
    clear
    echo -e "${GREEN}${BOLD}"
    echo "â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—"
    echo "â•‘              MINERAÃ‡ÃƒO RANDOMX ATIVA - 100x MAIS RÃPIDO          â•‘"
    echo "â•‘              LOGS EM TEMPO REAL - CTRL+C PARA PARAR              â•‘"
    echo "â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•"
    echo -e "${NC}"
    echo ""
    echo -e "${CYAN}Algoritmo:${NC} RandomX (Monero) - 100x mais rÃ¡pido que Bitcoin"
    echo -e "${CYAN}Pool:${NC} $pool_name"
    echo -e "${CYAN}URL:${NC} $pool_url"
    echo -e "${CYAN}Carteira:${NC} ${NICEHASH_WALLET:0:25}..."
    echo -e "${CYAN}Threads:${NC} $THREADS"
    echo ""
    
    god_log "GOD" "Mining" "=== INICIANDO OPERAÃ‡ÃƒO DIVINA DE MINERAÃ‡ÃƒO RANDOMX ==="
    god_log "INFO" "Mining" "Pool: $pool_name"
    god_log "INFO" "Mining" "URL: $pool_url"
    god_log "INFO" "Mining" "Algoritmo: RandomX (100x mais rÃ¡pido)"
    
    # Verificar minerador
    if [ ! -f "$BIN_DIR/xmrig" ]; then
        god_log "INFO" "Mining" "XMRig nÃ£o encontrado. Instalando..."
        install_xmrig || {
            god_log "ERROR" "Mining" "Falha na instalaÃ§Ã£o do XMRig"
            discord_log "Falha na instalaÃ§Ã£o" "NÃ£o foi possÃ­vel instalar o XMRig" "16711680" ""
            echo ""
            echo -e "${YELLOW}Pressione ENTER para voltar ao menu${NC}"
            read -r
            return 1
        }
    fi
    
    # Enviar notificaÃ§Ã£o Discord
    local earnings=$(calculate_earnings "8.0")
    discord_log "ğŸš€ MINERAÃ‡ÃƒO RANDOMX INICIADA" "Pool: $pool_name\nThreads: $THREADS\nEstimativa: R\$${earnings}/dia" "65280" ""
    
    # Criar configuraÃ§Ã£o
    cat > "$TEMP_DIR/config_randomx.json" << EOF
{
    "autosave": false,
    "cpu": {
        "enabled": true,
        "max-threads-hint": $THREADS,
        "rx/0": true
    },
    "donate-level": 0,
    "log-file": "$LOG_DIR/xmrig.log",
    "pools": [
        {
            "algo": "rx/0",
            "url": "$pool_url",
            "user": "$NICEHASH_WALLET",
            "pass": "x",
            "nicehash": true,
            "keepalive": true,
            "tls": false
        }
    ],
    "print-time": 5
}
EOF
    
    # Executar minerador em primeiro plano
    "$BIN_DIR/xmrig" -c "$TEMP_DIR/config_randomx.json" 2>&1 | tee -a "$LOG_DIR/miner.log" | while read line; do
        
        # Detectar hashrate
        if [[ "$line" =~ ([0-9]+\.[0-9]+)\ (kH/s|KH/s) ]]; then
            local hr="${BASH_REMATCH[1]} ${BASH_REMATCH[2]}"
            god_log "HASHRATE" "Miner" "âš¡ $hr"
            
            # Calcular estimativa em reais
            local brl=$(calculate_earnings "${BASH_REMATCH[1]}")
            echo -e "${GREEN}â†’ R\$${brl}/dia estimado${NC}"
        fi
        
        # Detectar shares
        if [[ "$line" == *"accepted"* ]]; then
            ((TOTAL_SHARES++))
            god_log "SHARE" "Miner" "âœ… Share aceito! Total: $TOTAL_SHARES"
        fi
        
        if [[ "$line" == *"rejected"* ]]; then
            ((TOTAL_REJECTED++))
            god_log "SHARE" "Miner" "âŒ Share rejeitado! Total: $TOTAL_REJECTED"
        fi
    done
    
    local exit_code=$?
    
    echo ""
    echo -e "${YELLOW}â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•${NC}"
    echo ""
    
    if [ $exit_code -eq 0 ] || [ $exit_code -eq 130 ]; then
        god_log "INFO" "Mining" "MineraÃ§Ã£o RandomX encerrada pelo usuÃ¡rio"
        discord_log "â¹ï¸ MINERAÃ‡ÃƒO RANDOMX ENCERRADA" "Total shares: $TOTAL_SHARES\nTempo ativo: $(( ($(date +%s) - START_TIME) / 60 )) minutos" "16711680" ""
    else
        god_log "ERROR" "Mining" "MineraÃ§Ã£o RandomX encerrada com erro (cÃ³digo: $exit_code)"
        discord_log "âŒ MINERAÃ‡ÃƒO RANDOMX ENCERRADA COM ERRO" "CÃ³digo: $exit_code\nVerifique os logs." "16711680" ""
    fi
    
    echo ""
    echo -e "${YELLOW}Pressione ENTER para voltar ao menu principal${NC}"
    read -r
}

# =============================================================================
# AUTO POOL SWITCH (a cada 30 minutos)
# =============================================================================
auto_pool_switch() {
    while true; do
        sleep 1800  # 30 minutos
        
        god_log "POOL" "AutoSwitch" "Verificando pools RandomX disponÃ­veis..."
        
        local best_pool=$(find_best_pool)
        local current_pool=$(cat "$CONFIG_DIR/current_pool" 2>/dev/null || echo "none")
        
        if [ "$best_pool" != "$current_pool" ] && [ -n "$best_pool" ]; then
            god_log "POOL" "AutoSwitch" "Nova pool RandomX disponÃ­vel: $best_pool"
            echo "$best_pool" > "$CONFIG_DIR/current_pool"
            
            discord_log "ğŸ”„ AUTO POOL SWITCH RANDOMX" "Pool alterada para: $best_pool\nLatÃªncia otimizada." "65280" ""
            
            # Se estiver minerando, reiniciar com nova pool
            if pgrep -f xmrig >/dev/null; then
                god_log "INFO" "AutoSwitch" "Reiniciando mineraÃ§Ã£o com nova pool..."
                pkill -f xmrig
                sleep 5
                start_mining_randomx "$best_pool"
            fi
        fi
        
        LAST_POOL_SWITCH=$(date +%s)
    done
}

# =============================================================================
# MONITOR DE TEMPERATURA
# =============================================================================
temperature_monitor() {
    while true; do
        sleep 60
        
        if [ -f "/sys/class/thermal/thermal_zone0/temp" ]; then
            local temp=$(($(cat /sys/class/thermal/thermal_zone0/temp) / 1000))
            
            if [ $temp -gt 80 ]; then
                god_log "WARN" "Temperature" "Temperatura crÃ­tica: ${temp}Â°C!"
                discord_log "âš ï¸ ALERTA DE TEMPERATURA RANDOMX" "Temperatura: ${temp}Â°C\nReduza a carga do dispositivo." "16744192" ""
                
                # Reduzir threads pela metade
                if pgrep -f xmrig >/dev/null; then
                    pkill -f xmrig
                    sleep 5
                    THREADS_HALF=$((THREADS / 2))
                    [ $THREADS_HALF -lt 1 ] && THREADS_HALF=1
                    
                    god_log "INFO" "Temperature" "Reduzindo threads para $THREADS_HALF"
                    export THREADS=$THREADS_HALF
                    local pool=$(cat "$CONFIG_DIR/current_pool" 2>/dev/null || echo "NiceHash RandomX Auto")
                    start_mining_randomx "$pool"
                fi
            fi
        fi
    done
}

# =============================================================================
# ENVIAR HEARTBEAT (a cada hora)
# =============================================================================
send_heartbeat() {
    while true; do
        sleep 3600  # 1 hora
        
        local uptime=$(( $(date +%s) - START_TIME ))
        local hours=$((uptime / 3600))
        local minutes=$(((uptime % 3600) / 60))
        local earnings=$(calculate_earnings "$CURRENT_HASHRATE")
        
        local fields='{
            "name": "Tempo ativo",
            "value": "'"${hours}h ${minutes}m"'",
            "inline": true
        },{
            "name": "Shares",
            "value": "'"$TOTAL_SHARES"'",
            "inline": true
        },{
            "name": "Hashrate atual",
            "value": "'"${CURRENT_HASHRATE:-0} KH/s"'",
            "inline": true
        },{
            "name": "Ganhos/dia",
            "value": "'"R\$${earnings}"'",
            "inline": true
        }'
        
        discord_log "ğŸ’“ HEARTBEAT - MODO DEUS RANDOMX" "Sistema operacional" "65280" "$fields"
        god_log "INFO" "Heartbeat" "Heartbeat enviado - Uptime: ${hours}h ${minutes}m | Ganhos: R\$${earnings}/dia"
    done
}

